    "version": "0.2.0",
    "configurations": [
        {
            "name": "debug dev",
            "request": "launch",
            "type": "dart",
            "customTool": "${workspaceFolder}/scripts/flutter_run_wrapper.sh",
            "preLaunchTask": "generate dart-defines",
            "args": [
                "--dart-define=DEV_MODE=true"
            ]
        },
        {
            "name": "debug",
            "request": "launch",
            "type": "dart",
            "customTool": "${workspaceFolder}/scripts/flutter_run_wrapper.sh",
            "preLaunchTask": "generate dart-defines"
        },
        {
            "name": "release dev",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "customTool": "${workspaceFolder}/scripts/flutter_run_wrapper.sh",
            "preLaunchTask": "generate dart-defines",
            "args": [
                "--dart-define=DEV_MODE=true"
            ]
        },
        {
            "name": "release",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "customTool": "${workspaceFolder}/scripts/flutter_run_wrapper.sh",
            "preLaunchTask": "generate dart-defines"
        },
    ]
}
